{
    "name": "uiibevy/flutzig",
    "description": "Use your laravel named routes in Flutter.",
    "type": "library",
    "keywords": [
        "laravel",
        "routes",
        "flutter",
        "dart",
        "flutzig"
    ],
    "homepage": "https://github.com/uiibevy/flutzig",
    "license": "MIT",
    "authors": [
        {
            "name": "Hicham SADDEK",
            "email": "hichamsaddek.stcnet@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "laravel/framework": ">=9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0 || ^8.0 || ^9.0",
        "pestphp/pest": "^2.3",
        "pestphp/pest-plugin-drift": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "Uiibevy\\Flutzig\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uiibevy\\Flutzig\\FlutzigServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
